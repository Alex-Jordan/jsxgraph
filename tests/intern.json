{
    "proxyPort": 9000,
    "proxyUrl": "http://localhost:9000/",
    "environments": [
            { "browserName": "chrome"}
        ],

    "maxConcurrency": 3,
    "loaderOptions": {
        "packages": [
            { "name": "JSXGraph", "location": "src" }
        ],
        "baseUrl": "src/",
        "paths": {
            "tests": "../tests/",
            "node": "../node_modules/"
        }
    },

    "suites": [
        "tests/unit/jxg",
        "tests/unit/utils/base64",
        "tests/unit/utils/color",
        "tests/unit/utils/encoding",
        "tests/unit/utils/type",
        "tests/unit/utils/uuid"
    ]
}
